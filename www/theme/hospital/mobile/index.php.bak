	<script>
				var bx_idx;
		var bna_img = $(".bna_img ul").bxSlider({
			infiniteLoop : false,
			pagerCustom : '.bna_thumb',
			onSliderLoad :  function(obj){
				bx_idx = obj;
			},
			onSlideAfter : function(obj){
				$(".bna_img li > img").hide();
				$(".bna_img li > .on").show();
				bx_idx = $(obj).index();
				
			}
		});

		
		// 각도버튼 클릭시 해당이미지 show
		$(".bna_img_btn span").click(function(){
			if ( $(this).attr("class") != "on" ){
				$(".bna_img_btn span").removeClass("on");
				$(".bna_img li > img").hide();

				$(this).addClass("on");
				$(".bna_img li").eq(bx_idx).find("img[alt='"+ $(this).text() +"']").stop().fadeIn("fast");
			}
		});
		var curIdx = 0;
		// 버튼 클릭시 초기셋팅
		function work(){
			$(".bna_img_btn span").hide();
			$(".bna_wrap .bna_img ul li").eq(curIdx).find("img").each(function(idx, item){
				$(".bna_img_btn span")
					.filter(
						function(){
							if( $(this).text() == $(item).attr("alt"))
								return this;
				}).show();
			});

			$(".bna_img_btn span").removeClass("on");
			$(".bna_img li img span").hide();

			$(".bna_img_btn span").eq(0).addClass("on");
			$(".bna_img li img").each(function(){
				if ( $(this).index() == 0 ){
					//alert($(this).parent("li").attr("data-no"));
					$(this).show();
				}
			});
		}
		work();
		$(".bna_img .bx-controls-direction a, .bna_thumb a").click(function(){
			if($(this).hasClass("bx-prev")){
				if( 1 <= curIdx ){
					curIdx--;
					work();
				}
			}else if($(this).hasClass("bx-next")){
				if( curIdx < 3-1 ){
					curIdx++;
					work();
				}
			}else{
				curIdx = $(this).attr("data-slide-index");
				work();
			}
		});

		// 영상보기 클릭시 스크롤 이동
		$(".bna_more img").click(function(){
			$("html, body").stop().animate({scrollTop: $(this).offset().top - 30 });
		});
			
	</script>
	
