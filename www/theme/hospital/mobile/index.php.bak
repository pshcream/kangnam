<script>
    
        function openPlay(evt, playName) {
  // Declare all variables
  var i, ddtv_view, model_list;

  // Get all elements with class="tabcontent" and hide them
  ddtv_view = document.getElementsByClassName("ddtv_view");
  for (i = 0; i < ddtv_view.length; i++) {
    ddtv_view[i].style.display = "none";
  }

  // Get all elements with class="tablinks" and remove the class "active"
  model_list = document.getElementsByClassName("model_list");
  for (i = 0; i < model_list.length; i++) {
    model_list[i].className = model_list[i].className.replace(" on", "");
  }

  // Show the current tab, and add an "active" class to the button that opened the tab
  document.getElementById(playName).style.display = "inline-block";
  evt.currentTarget.className += " on";
}

    
    </script>


