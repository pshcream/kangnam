@media screen and (min-width: 640px){
 .main_visual .titleWrap h2 {font-size: 20px;}   
.main_visual .titleWrap span {font-size: 48px;}    
}
.main_visual .swiper-button-next, .main_visual .swiper-button-prev {
position: absolute; top: 50%; margin-top: -20px; width: 40px; height: 40px; background: none; font-size: 30px; z-index: 101; }
.main_visual .swiper-button-next {right: 50%; margin-right: -590px; outline: none;}
.main_visual .swiper-button-next:after {content: '\e940'; font-family: xeicon; color: #fff; font-size: 40px; position: absolute; top: 0; left: 0;}
.main_visual .swiper-button-prev {left: 50%; margin-left: -590px; outline: none;}
.main_visual .swiper-button-prev:after {content: '\e93d'; font-family: xeicon; color: #fff; font-size: 40px; position: absolute; top: 0; left: 0;}
