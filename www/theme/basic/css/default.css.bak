.tab::-webkit-scrollbar-thumb { border-radius: 16px; background:black;}
.tab::-webkit-scrollbar-track {background:white;}

/* Style the buttons that are used to open the tab content */
.tab button {
  background-color: black;
  float: left;
  border: none;
  outline: none;
  cursor: pointer;
  margin: 14px 0;
  transition: 0.3s;
        width: calc(100% - 30px);
}

